<Page x:Class="Project_04.ResultPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Project_04"
      mc:Ignorable="d" 
      d:DesignHeight="352" d:DesignWidth="1014.4"
      Title="ResultPage">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Label x:Name="PeriodInfo" Grid.Column="0" Grid.Row="4" Margin="5" HorizontalContentAlignment="Center"/>
        <Label x:Name="AleInfo" Grid.Column="0" Grid.Row="0" Margin="5" HorizontalContentAlignment="Center"/>

        <Label x:Name="GrowthInfo" Grid.Column="2" Grid.Row="0" Margin="5" HorizontalContentAlignment="Center"/>
        <Label x:Name="DeclineInfo" Grid.Column="2" Grid.Row="1" Margin="5" HorizontalContentAlignment="Center"/>


    </Grid>
</Page>
