<Window x:Class="Project_05.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_05"
        mc:Ignorable="d"
        Title="Счастливый билет" Height="396" Width="800" Icon="Resources/Logo.ico">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="48*"/>
            <RowDefinition Height="120*"/>
            <RowDefinition Height="83*"/>
            <RowDefinition Height="117*"/>
        </Grid.RowDefinitions>

        <Label Content="Проверка на счастливый билет" Grid.Column="1" Grid.Row="0" Grid.ColumnSpan="3" FontSize="18" Margin="5,5,5,-15" Width="274"/>
        <Frame NavigationUIVisibility="Hidden" x:Name="MainWindowFrame" Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="5" Grid.RowSpan="3"/>
    </Grid>
</Window>
